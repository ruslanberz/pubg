@model PUBG.Models.CabinetVw
@{
    ViewData["Title"] = "Cabinet";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-xl-8 col-lg-8 col-md-12 col-sm-12 col-12 col-xs-12 offset-xl-2 offset-lg-2" style="margin-top:80px">
            <div class="card text-center bg-transparent" style="height:500px">
                <div class="card-header">
                   <img src="~/images/profile.png" width="50"/>
                </div>
                <div class="card-body">
                    <h5 class="card-title">@Model.User.PubgUsername</h5>
                    <p class="card-text">Sezon: @Model.SeasonName </p>
                    <p class="card-text">Oyun nəticəsiniz: @Model.Say </p>
                    <p class="card-text">Xal nəticəsiniz: @Model.Xal </p>
                    <a href="~/home/imagehandle" class="btn btn-primary">Nəticələri yenilə</a>
                </div>
               
            </div>
        </div>
    </div>
</div>